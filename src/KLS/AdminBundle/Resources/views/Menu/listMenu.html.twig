{% for item in list %}
    <tr id="line-{{ item.id }}">
        <td>{{ item.name }}</td>
        <td class="text-center"><a href="{{ path('sabroso_admin_edit_menu', {'id': item.id}) }}"><span class="fa fa-edit"></span></a></td>
        <td class="text-center"><button type="button" data-toggle="modal" data-target="#{{ item.id }}" class="btn btn-xs btn-danger"><span class="fa fa-trash"></span></button></td>
    </tr>
{% endfor %}